<%= flash[:notice] %>

<h1>Login</h1>
<%= form_tag('/login') do %>
  <%= label_tag :email %>
  <%= text_field_tag :email, nil, id: "loginsignup" %>
  <%= label_tag :password %>
  <%= password_field_tag :password, nil, id: "loginsignup" %>
  <%= submit_tag "Login" %><%= link_to 'Or Create an Account', '/signup', class: 'button', id: 'signup-button'%>
<% end %>


