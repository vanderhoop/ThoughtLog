<h3><%= @user_first_name %>'s Entries</h3>
<!-- list of all entries -->
<% counter = 1 %>
<% @user_entries.each do |entry| %>
  <%= link_to "Entry #{counter}", "/users/#{session[:user_id]}/entries/#{entry.id}" %></br>
  <p>Created about <%= time_ago_in_words(entry.created_at) %> ago</p>
<% counter +=1 %>
<% end %>
<%= link_to 'Create New Entry', "/users/#{session[:user_id]}/entries/new", class:'button' %>
